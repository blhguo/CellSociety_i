	</cell>
	
	<cell>
		<type>alive</type>
		<x>1</x>
		<y>3</y>
	</cell>
	
	<cell>
		<type>alive</type>
		<x>2</x>
		<y>3</y>
	</cell>
	
	<cell>
		<type>alive</type>
		<x>3</x>
		<y>3</y>
	</cell>
	
</simulation>